        code : |1+
         TacheDevientParent est une tachegantt
         
         nIndiceSélection est un entier = TableSelect(TABLE_Gantt)
         // Une ligne doit être sélectionnée
         SI nIndiceSélection <= 0 ALORS RETOUR
         
         // Déplace la branche courante en fils de la branche précédente ou au même niveau (si la branche précédente est dans un niveau inférieur)
         nIndiceParentSélection est un entier = TableRécupèreParent(TABLE_Gantt, nIndiceSélection)
         nIndiceParentPrécédent est un entier = TableRécupèreParent(TABLE_Gantt, nIndiceSélection-1)
         nIndiceNouveauParent est un entier
         nIndiceLigne est un entier
         
         // Si les deux lignes sont déjà "frères", passe la sélection en fils de la ligne précédente
         si nIndiceParentSélection = nIndiceParentPrécédent alors
         	
         	// Récupère la tâche qui va devenir cumulative : il faut prévenir si c'est un jalon (et pouvoir annuler)
         	TacheDevientParent = GanttRécupèreTâche(COL_Gantt,COL_IDTâche[nIndiceSélection-1])
         	si TacheDevientParent.jalon alors
         		Si ouinon(non, "Attention : la tâche précédente de même niveau est un jalon et va être transformée en tâche cumulative. Êtes-vous sûr de vouloir modifier la hiérarchie de la tâche sélectionnée ?") = non alors retour
         	FIN
         	
         	// Déplace la branche en fils de la branche précédente
         	TableDéplaceBranche(TABLE_Gantt, nIndiceSélection, nIndiceSélection-1, tdEnFils)
         	
         SINOn // Si les deux lignes ne sont pas au même niveau
         	
         	// Il faut trouver le fils précédent sur le même niveau 
         	nIndiceFilsPrécedent est un entier
         	nIndiceFils est un entier = TableRécupèreFils(TABLE_Gantt, nIndiceParentSélection, taPremier)
         	TANTQUE nIndiceFils <> -1
         		
         		// Si le fils parcouru est celui sélectionné
         		SI nIndiceFils = nIndiceSélection ALORS
         			
         			si nIndiceFilsPrécedent = 0 alors retour
         			
         			// On connait le fils précédent de même niveau que la sélection : il faut lister les fils jusqu'au dernier
         			nIndiceFils = TableRécupèreFils(TABLE_Gantt, nIndiceFilsPrécedent, taPremier)
         			TANTQUE nIndiceFils <> -1
         				nIndiceLigne = nIndiceFils
         				nIndiceFils = TableRécupèreFils(TABLE_Gantt, nIndiceFilsPrécedent, taSuivant)
         			FIn
         			
         			// Déplace la branche en frère de la branche trouvée
         			TableDéplaceBranche(TABLE_Gantt, nIndiceSélection, nIndiceLigne, tdenfrere)
         			
         			// Fin du traitement
         			SORTIR
         			
         		FIN
         		
         		// Mémorise le fils comme précédent
         		nIndiceFilsPrécedent = nIndiceFils
         		
         		// Passe au fils précédent
         		nIndiceFils = TableRécupèreFils(TABLE_Gantt, nIndiceParentSélection, taSuivant)
         		
         	FIN
         	
         FIN
         
         // Sélectionne la ligne dans sa nouvelle position
         TableSelectPlus(TABLE_Gantt, nIndiceSélection)
         
         // Mémorise le nouveau parent de la branche sélectionnée
         nIndiceNouveauParent = TableRécupèreParent(TABLE_Gantt, nIndiceSélection)
         
         // Passe la tâche parent en cumulative
         PasseTâcheEnCumulative(nIndiceNouveauParent)
         
         // Met à jour les valeurs de l'ancienne tâche parent
         CalculTâchesParents(nIndiceParentSélection)
         // Met à jour les valeurs pour la tâche
         CalculTâchesParents(nIndiceSélection)
         // Met à jour les informations du diagramme de Gantt
         AfficheInfosGénéral()
        type : 18
      -
        type : 161
        enabled : false

     code : |1+
      // Résumé : Passe une tâche en jalon
      // Syntaxe :
      //PasseTâcheEnJalon (<nIndiceLigne> est entier)
      //
      // Paramètres :
      //	nIndiceLigne (entier) : Indice de la ligne
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PasseTâcheEnJalon(LOCAL nIndiceLigne est un entier)
      
      // Récupère la tâche parente
      TacheParent est une TâcheGantt <- TABLE_Gantt.COL_Gantt[COL_IDTâche[nIndiceLigne]]
      
      // - passe la tâche en non cumulative
      TacheParent.Cumulative = Faux
      // - passe la tâche en jalon
      TacheParent.jalon = vrai
      
      // - positionne des couleurs spécifiques
      TacheParent.CouleurAvancement = COULEUR_JALON
      TacheParent.CouleurFond = COULEUR_JALON
      
      // - modifie l'image de la ligne
      TABLE_Gantt[nIndiceLigne]..ImageDéroulée = IMG_Jalon
      TABLE_Gantt[nIndiceLigne]..ImageEnroulée = IMG_Jalon
      
      // - active la modification de la date
      COL_DateDébut[nIndiceLigne]..Etat = actif
      
      // - empêche la modification des autres colonnes
      COL_Durée[nIndiceLigne]..Etat = Grisé
      COL_Avancement[nIndiceLigne]..Etat = Grisé
      
      // - vide les colonnes qui n'ont pas de sens
      COL_Durée[nIndiceLigne] = ""
      COL_Avancement[nIndiceLigne] = ""

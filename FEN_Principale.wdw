            PROCEDURE Suppression(tgSupprimée est une TâcheGantt)
            
            // Confirmation de suppression
            si ouinon(non, "Supprimer la tâche """+tgSupprimée.titre+""" ?") = non alors renvoyer faux
            
            // Si c'est une tâche standard
            si tgSupprimée.Cumulative = faux ALORS
            	
            	// Mémorise l'indice de la tâche parent
            	nIndiceParent est un entier = TableRécupèreParent(TABLE_Gantt, tgSupprimée.Ligne)
            	
            	// Supprime la ligne
            	tablesupprime(TABLE_Gantt, tgSupprimée.ligne)
            	
            	// Si une tâche parent existe
            	SI nIndiceParent > 0 ALORS
            		// Si cette tâche n'a plus de fils
            		SI TableRécupèreFils(TABLE_Gantt, nIndiceParent, tapremier) = -1 ALORS
            			// La tâche parent devient une tâche standard
            			PasseTâcheEnStandard(nIndiceParent)
            		FIN
            	FIN
            	
            SINON // Si la tâche est cumulative
            	
            	tabIndice est un tableau d'entiers
            	// Il faut supprimer l'indentation des fils direct
            	nIndiceFils est un entier = TableRécupèreFils(TABLE_Gantt, tgSupprimée.Ligne, tapremier)
            	TANTQUE nIndiceFils <> -1
            		// Mémorise la ligne
            		ajoute(tabIndice, nIndiceFils)	
            		// Passe au fils précédent
            		nIndiceFils = TableRécupèreFils(TABLE_Gantt, tgSupprimée.Ligne, tasuivant)
            	FIN
            	
            	// Parcours le tableau à l'envers (pour que le déplacement des lignes ne perde pas l'ordre initial)
            	pour tout element nIndice de tabIndice depuisfin
            		nIndiceParent est un entier = TableRécupèreParent(TABLE_Gantt, nIndice)
            		
            		// La ligne doit avoir un parent
            		SI nIndiceParent > 0 ALORS	
            			// Déplace la branche courante en fils de la branche précédente
            			TableDéplaceBranche(TABLE_Gantt, nIndice, nIndiceParent, tdEnFrère)
            		FIN
            		
            	FIN
            	
            	// Supprime la ligne
            	TableSupprime(TABLE_Gantt, tgSupprimée.Ligne)
            	
            FIN
            
            // Lance le calcul (durée, début, avancement) des tâches cumulatives parents
            CalculTâchesParents(tgSupprimée.Ligne)
            // Met à jour toutes les tâches si nécessaire
            MetAJourTâchesSuiteRéorganisationAuto()
            // Met à jour les informations du diagramme de Gantt
            AfficheInfosGénéral()
            
            // Modifie l'état des boutons
            AdapteBoutons()
            
            // Supprime la tâche
            RENVOYER Vrai

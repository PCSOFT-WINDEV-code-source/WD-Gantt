        code : |1+
         nIndiceSélection est un entier = TableSelect(TABLE_Gantt)
         
         // Une ligne doit être sélectionnée
         si nIndiceSélection <= 0 alors retour
         
         // Il faut monter sur le niveau courant uniquement
         tabIndiceNiveau est un tableau d'entiers
         
         // Récupère l'indice parent
         nIndiceParent est un entier = TableRécupèreParent(TABLE_Gantt, nIndiceSélection)
         // Liste les fils de ce parent
         nNumFils est un entier
         nIndiceFilsPrécedent est un entier
         nIndiceFils est un entier = TableRécupèreFils(TABLE_Gantt, nIndiceParent, taPremier)
         TANTQUE nIndiceFils <> -1
         	
         	// Si le fils parcouru est celui sélectionné
         	si nIndiceFils = nIndiceSélection ALORS
         		
         		// Si la branche sélectionnée devient le nouveau premier fils
         		si nNumFils = 1 ALORS
         			// Déplace la ligne vers le haut
         			nIndiceFils = TableDéplaceBranche(TABLE_Gantt, nIndiceSélection, nIndiceParent, tdenfils)
         		SINON
         			si nIndiceFilsPrécedent <= 1 alors retour
         			
         			// Déplace la ligne vers le haut
         			nIndiceFils = TableDéplaceBranche(TABLE_Gantt, nIndiceSélection, tabIndiceNiveau[tabIndiceNiveau..Occurrence-1], tdEnFrère)
         		FIN
         		
         		// Sélectionne la ligne dans sa nouvelle position
         		TableSelectPlus(TABLE_Gantt, nIndiceFils)
         		
         		// Fin du traitement
         		SORTIR
         		
         	FIN
         	// Mémorise l'indice
         	ajoute(tabIndiceNiveau, nIndiceFils)
         	
         	// Mémorise le fils comme précédent
         	nIndiceFilsPrécedent = nIndiceFils
         	nNumFils++
         	// Passe au fils précédent
         	nIndiceFils = TableRécupèreFils(TABLE_Gantt, nIndiceParent, taSuivant)
         	
         FIN

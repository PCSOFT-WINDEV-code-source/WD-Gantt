     name : PasseTâcheEnStandard
     procedure_id : 1334446411886900632
     type_code : 14
     code : |1-
      // Résumé : Passe une tâche en standard
      // Syntaxe :
      //PasseTâcheEnStandard (<nIndiceLigne> est entier)
      //
      // Paramètres :
      //	nIndiceLigne (entier) : Indice de la ligne
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PasseTâcheEnStandard(LOCAL nIndiceLigne est un entier)
      
      // Récupère la tâche parente
      UneTache est une TâcheGantt <- TABLE_Gantt.COL_Gantt[COL_IDTâche[nIndiceLigne]]
      
      // - passe la tâche en non cumulative
      UneTache.Cumulative = Faux
      
      // - passe la tâche en non jalon
      UneTache.Jalon = faux
      
      // - positionne des couleurs spécifiques
      UneTache.CouleurAvancement = COULEUR_AVANCEMENT
      UneTache.CouleurFond = COULEUR_FOND
      
      // - pas d'image pour les tâches
      TABLE_Gantt[nIndiceLigne]..LargeurImageDossier = 0
      
      // - rétablit la modification des colonnes 
      COL_DateDébut[nIndiceLigne]..Etat = Actif
      COL_Durée[nIndiceLigne]..Etat = Actif
      COL_Avancement[nIndiceLigne]..Etat = Actif
      
      // - remplit les colonnes
      COL_Durée[nIndiceLigne] = UneTache.DuréeEnJour
      COL_Avancement[nIndiceLigne] = UneTache.avancement
     type : 458752
   -
     name : PasseTâcheEnCumulative
     procedure_id : 1334446411886966168
     type_code : 14
     code : |1+
      // Résumé : Passe une tâche en cumulative
      // Syntaxe :
      //PasseTâcheEnCumulative (<nIndiceLigne> est entier)
      //
      // Paramètres :
      //	nIndiceLigne (entier) : Indice de la ligne
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PasseTâcheEnCumulative(LOCAL nIndiceLigne est un entier)
      
      // Récupère la tâche parente
      UneTache est une TâcheGantt <- TABLE_Gantt.COL_Gantt[COL_IDTâche[nIndiceLigne]]
      
      // - passe la tâche en cumulative
      UneTache.Cumulative = Vrai
      
      // - passe la tâche en non jalon
      UneTache.Jalon = faux
      
      // - positionne des couleurs spécifiques
      UneTache.CouleurAvancement = (INT_AvancementTacheCumulative ? COULEUR_AVANCEMENT SINON COULEUR_CUMULATIVE)
      UneTache.CouleurFond = (INT_AvancementTacheCumulative ? COULEUR_FOND SINON COULEUR_CUMULATIVE)
      
      // - modifie l'image de la ligne
      TABLE_Gantt[nIndiceLigne]..ImageDéroulée = IMG_Répertoire_ouvert
      TABLE_Gantt[nIndiceLigne]..ImageEnroulée = IMG_Répertoire_fermé
      
      // - empêche la modification des colonnes (à l'exception du titre)
      COL_DateDébut[nIndiceLigne]..Etat = Grisé
      COL_Durée[nIndiceLigne]..Etat = Grisé
      COL_Avancement[nIndiceLigne]..Etat = Grisé
      
      // - remplit les colonnes
      COL_Durée[nIndiceLigne] = UneTache.DuréeEnJour
      COL_Avancement[nIndiceLigne] = UneTache.Avancement
      
      // Détermine les valeurs pour cette tâche
      CalculTâchesParents(nIndiceLigne)
     type : 458752
   -
     name : CalculTâchesParents
     internal_properties : CAAAAAgAAADaVcYQHIbyoVREO9FmuBPaPcL2gt4V6fZmeIgZcbaKn1C1G4JJoAQhzpbSyPZUAWom3t77VmS5E9YymRaUgoSlhY7wvOCkG2akzM5ReZaVxmtQMO5i18RTIY7ZAdwZ+ZwIz+C0iXomUUXIw+wd9QjpzkTKZ1EqDSLFCqldMvriG5dZAaPAE1rHvRIkBLnFMEia1GcGGbI=
     procedure_id : 1334446411887031704
     type_code : 14
     code : |1+
      // Résumé : Calcul les informations des tâches parents (début, durée, avancement)
      // Syntaxe :
      //CalculTâchesParents (<nIndiceLigne> est entier)
      //
      // Paramètres :
      //	nIndiceLigne (entier) : Indice de la ligne
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE CalculTâchesParents(LOCAL nIndiceLigne est un entier)
      
      // Indice doit être valide
      SI nIndiceLigne <= 0 _OU_ nIndiceLigne > TABLE_Gantt..Occurrence ALORS RETOUR
      
      
      nIndiceParent est un entier
      cTâche est un TâcheGantt
      cTâcheParcourue est un TâcheGantt
      nDurée est un entier
      dhDébut est une dateheure
      dhDébutTâche est une dateheure
      dhFin est une dateheure
      dhFinTâche est une dateheure
      nNbFils est un entier
      nTotalAvancement est un entier
      
      
      // Récupère la tâche de la ligne demandée
      cTâche <- COL_Gantt[COL_IDTâche[nIndiceLigne]]
      // Si la tâche n'est pas cumulative
      SI cTâche..Cumulative = Faux ALORS
      	// Il faut se positionner sur la tâche parent
      	nIndiceParent = TableRécupèreParent(TABLE_Gantt, nIndiceLigne)
      SINON
      	// Il faut se positionner sur la tâche
      	nIndiceParent = nIndiceLigne
      FIN
      
      // Remonte l'arborescence
      TANTQUE nIndiceParent > 0
      	
      	// Initialise les valeurs
      	dhDébut = ""
      	dhFin = ""
      	nDurée = 0
      	nTotalAvancement = 0
      	nNbFils = 0
      	
      	// Parcourt les fils de la ligne pour effectuer les calculs
      	nIndiceFils est un entier = TableRécupèreFils(TABLE_Gantt, nIndiceParent, taPremier)
      	TANTQUE nIndiceFils <> -1
      		// Charge la tâche	
      		cTâcheParcourue <- COL_Gantt[COL_IDTâche[nIndiceFils]]
      		
      		// Incrémente le nombre de fils (sans compter les jalons)
      		SI cTâcheParcourue..Jalon = Faux ALORS nNbFils++ 
      		
      		// Incrémente le total avancement
      		nTotalAvancement += Val(cTâcheParcourue..Avancement)
      		
      		// Mémorise le début et fin de la tâche
      		dhDébutTâche = cTâcheParcourue..DateDébut
      		dhFinTâche = cTâcheParcourue..DateFin
      		
      		// Pour que le calcul des tâches cumulatives soit correcte, la dateheure de fin est modifiée au jour précédent à 18h00
      		SI cTâcheParcourue..Jalon ALORS
      			dhFinTâche..Jour--
      			dhFinTâche..PartieHeure = "180000000"
      		FIN
      		
      		// Détermine le début et la fin(date minimale des tâches subordonnées)
      		dhDébut = (dhDébut = "00000000000000000" ? dhDébutTâche SINON Min(dhDébut, dhDébutTâche))
      		dhFin = (dhFin = "00000000000000000" ? dhFinTâche SINON Max(dhFin, dhFinTâche))
      		
      		// Passe au fils précédent
      		nIndiceFils = TableRécupèreFils(TABLE_Gantt, nIndiceParent, taSuivant)
      		
      	FIN
      
      	// Charge la tâche	
      	cTâche <- COL_Gantt[COL_IDTâche[nIndiceParent]]
      	
      	// Applique les valeurs
      	COL_Avancement[nIndiceParent] = Arrondi(nTotalAvancement / Max(nNbFils, 1))
      	cTâche..Avancement = COL_Avancement[nIndiceParent]
      	COL_DateDébut[nIndiceParent] = dhDébut
      	cTâche..DateDébut = COL_DateDébut[nIndiceParent]
      	
      	// La durée doit être calculée par incrémentation (pas de 1) tant que la date de fin de la dernière tâche n'est pas atteinte
      	// (de cette façon, la gestion des jours ouvrés et jours fériés est automatiquement réalisée par le champ Gantt)
      	cTâche..DuréeEnJour = 1
      	TANTQUE cTâche..DateFin < dhFin..PartieDate
      		cTâche..DuréeEnJour++
      	FIN		
      	COL_Durée[nIndiceParent] = cTâche..DuréeEnJour
      	
      	// Il faut se positionner sur la tâche parent
      	nIndiceParent = TableRécupèreParent(TABLE_Gantt, nIndiceParent)
      	
      FIN
      
      // Rafraîchit l'affichage
      MultitâcheRepeint(FEN_Principale)
     type : 458752

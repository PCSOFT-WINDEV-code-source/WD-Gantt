     name : AfficheInfosGénéral
     internal_properties : CAAAAAgAAABa1UaQj67HiWQEWwkWGNMatQKushbVISYugDDZaU6ypxBNu/qZyPQJ7oZiUJYEYUruHva7LrTxQ94qkTZcWvw91SbQdFAMCz40TP61HerhSm9MvLrOs1hnNVrVJYBt1fBsq8xwtTYaxdlEH3ARCWQNIhCWc10mAZZJtrVBpn5un5tF/Z8sr76rQS7QOP25TGkLVOeGmTI=
     procedure_id : 1334715418571207968
     type_code : 14
     code : |1-
      // Résumé : Affiche les informations générales du projet : début, fin, avancement
      // Syntaxe :
      //AfficheInfosGénéral ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheInfosGénéral()
      
      UneTâche est un tachegantt
      dDébut est une date = ""
      dFin est une date = ""
      nAvancement est un entier
      nNbElement est un entier
      
      
      // Parcourt la table
      pour toute ligne nIndice de TABLE_Gantt
      	
      	// Uniquement les premiers niveaux (tâches cumulatives contiennent les informations nécessaires)
      	si TableRécupèreParent(TABLE_Gantt, nIndice) > 0 alors continuer
      	
      	// Récupère la tâche de cette ligne
      	UneTâche <- COL_Gantt[COL_IDTâche[nIndice]]
      	
      	// Date de début
      	dDébut = (dDébut = "" ? UneTâche.DateDébut SINON min(dDébut, UneTâche.DateDébut))
      	
      	// Date de fin
      	dFin = (dFin = "" ? UneTâche.DateFin SINON Min(dFin, UneTâche.DateFin))
      	
      	// Avancement
      	nAvancement += UneTâche.Avancement
      	
      	// Un élément supplémentaire (ne pas compter les jalons qui n'ont pas d'avancement)
      	si UneTâche.jalon = faux alors nNbElement++
      	
      FIN
      
      // Remplit le libellé
      Si TABLE_Gantt..Occurrence = 0 ALORS
      	LIB_DétailsProjet = <§$0001§>
      SINON
      	LIB_DétailsProjet = chaineconstruit("Date de début : %1 | Date de fin : %2 | Avancement global : %3 %", gpolicegras(vrai)+dateverschaine(dDébut, "JJJJ JJ MMMM AAAA")+gpolicegras(faux), gPoliceGras(Vrai)+DateVersChaîne(dFin, "JJJJ JJ MMMM AAAA")+gPoliceGras(Faux), gPoliceGras(Vrai)+arrondi(nAvancement/max(1, nNbElement)))	
      FIN
     type : 458752
   -
     name : AjouteTâche
     internal_properties : CAAAAAgAAAD+eTocCbpjaVgYNyl29NuewfaGWs5hRQaiZIzxCdISE3zR28IJ7BCRInr2EB64eU6ikl5j3ihNAzrmdeZUJkx5qVK49NiQLxbAyMIHr2T7aPnWNuho/cLhrwTfZ8IXjy7exdamT5A85yvuWQJri6YLlIKIGaeIK1ALiLerIOCoVUWPN7XG8VAt2/DSYsfLHr/N6HMKBY4=
     procedure_id : 1334733508974000356
     type_code : 14
     code : |1+
      // Résumé : Ajoute une nouvelle tâche (ou un nouveau jalon)
      // Syntaxe :
      //AjouteTâche ( [<bJalon> est booléen])
      //
      // Paramètres :
      //	bJalon (booléen - valeur par défaut=0) : Vrai pour ajouter un jalon, Faux pour ajouter une tâche
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AjouteTâche(LOCAL bJalon est un booleen = faux)
      
      NouvelleTâche est un tachegantt
      stUneTache est un STInfosTâcheGantt
      
      
      // Définit la tâche
      NouvelleTâche.Avancement = 0
      NouvelleTâche.CouleurAvancement = (bJalon ? COULEUR_JALON SINON COULEUR_AVANCEMENT)
      NouvelleTâche.CouleurFond = (bJalon ? COULEUR_JALON SINON COULEUR_FOND)
      NouvelleTâche.Cumulative = Faux
      NouvelleTâche.DateDébut = DateDuJour()+"080000"
      si bJalon = faux alors NouvelleTâche.DuréeEnJour = 1
      NouvelleTâche.ID = DonneGUID()
      NouvelleTâche.Jalon = bJalon
      NouvelleTâche.Titre = (bJalon ? "Nouveau jalon" SINON "Nouvelle tâche ")+" "+DonneIdentifiant()
      
      // Mémorise la tâche
      stUneTache.sIDTâche = NouvelleTâche.ID
      stUneTache.Tâche = NouvelleTâche
      stUneTache.nIndiceParent = 0
      
      // Ajoute la ligne "tâche"
      nIndice est un entier = Tableajouteligne(TABLE_Gantt, NouvelleTâche.ID, NouvelleTâche.Titre, NouvelleTâche.DateDébut, (bJalon ? "" SINON NouvelleTâche.DuréeEnJour), (bJalon ? "" SINON NouvelleTâche.Avancement), NouvelleTâche)
      // Force le style et les informations
      si bJalon ALORS	PasseTâcheEnjalon(nIndice) SINON PasseTâcheEnStandard(nIndice)
      
      // Met à jour les valeurs des tâches parents
      CalculTâchesParents(nIndice)
      
      // Met à jour les informations du diagramme de Gantt
      AfficheInfosGénéral()
      
      // Modifie l'état des boutons
      AdapteBoutons()
      
      // Se positionne sur la ligne
      TableSelectPlus(TABLE_Gantt, nIndice)
      // Rentre en saisie pour modifier le titre
      RepriseSaisie(TABLE_Gantt.COL_Titre)
     type : 458752
   -
     name : MetAJourDiagramme
     procedure_id : 1334745650847406931
     type_code : 14
     code : |1-
      // Résumé : Met à jour le diagramme
      // Syntaxe :
      //MetAJourDiagramme ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MetAJourDiagramme()
      
      cTâcheTable est une tachegantt
      stTâcheParcourure est un STInfosTâcheGantt
      
      
      // Vide la variable
      variableraz(gstDiagrammeGantt)
      
      // Mémorise les tâches
      pour toute ligne nIndice de TABLE_Gantt
      	
      	// Récupère la tâche de la table
      	cTâcheTable <- COL_Gantt[COL_IDTâche[nIndice]]
      	
      	// Vide la variable de mémorisation
      	VariableRAZ(stTâcheParcourure)
      	// Mémorise les informations
      	stTâcheParcourure.sIDTâche = cTâcheTable.ID
      	stTâcheParcourure.nIndiceParent = TableRécupèreParent(TABLE_Gantt, nIndice)
      	stTâcheParcourure.Tâche = cTâcheTable
      	// Ajoute cette tâche
      	ajoute(gstDiagrammeGantt.tabListeTâches, stTâcheParcourure)
      	
      FIN
      
      // Mémorise les liens
      gstDiagrammeGantt.tabListeLiens = GanttListeLien(COL_Gantt)
     type : 458752
   -
     name : ChargeFichier
     internal_properties : CAAAAAgAAABmtpIE+g6zP4uEzORv0mv7sdCHlEOpEmN3qcZ5mW/9vltqL0UUMRdJXPO61BByF4FlLFeH8TgAV5FjwUITdLks2XYi40JMUoiTrMtXlXdJjhGu9qWH+AinTGiQYzdEFR9AfTBJDJHoAx4iMmCQ0FSeinIQ8Mit5erK0I73hX+0CtxA6m9qeVOus9egVxuNBIvJoWPfgHKicttba+XL+rbqPikl
     procedure_id : 1334844632664475318
     type_code : 14
     code : |1+
      // Résumé : Charge un fichier wdgantt
      // Syntaxe :
      //ChargeFichier (<sFichier>)
      //
      // Paramètres :
      // 	sFichier : <indiquez ici le rôle de sFichier>
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ChargeFichier(sFichier)
      
      nIndiceLigne est un entier
      cUneTache est une tachegantt
      stUneInfoGantt est un STInfosTâcheGantt
      
      
      TABLE_Gantt..AffichageActif = faux
      
      // Vide la structure
      Variableraz(gstDiagrammeGantt)
      // Vide la table
      GanttSupprimeTout(COL_Gantt)
      TableSupprimeTout(TABLE_Gantt)
      
      // Charge le fichier
      bufContenu est un Buffer = fchargebuffer(sFichier)
      
      // Désérialise le diagramme des  tâches au format XML
      Désérialise(gstDiagrammeGantt, bufContenu, psdXML)
      
      // Parcourt le tableau pour ajouter les tâches
      POUR TOUT ELEMENT stUneInfoGantt DE gstDiagrammeGantt.tabListeTâches
      	
      	SI stUneInfoGantt.nIndiceParent = 0 ALORS
      		// Ajoute la ligne
      		nIndiceLigne = TableAjouteLigne(TABLE_Gantt, stUneInfoGantt.Tâche.ID, stUneInfoGantt.Tâche.Titre, stUneInfoGantt.Tâche.DateDébut, stUneInfoGantt.Tâche.DuréeEnJour, stUneInfoGantt.Tâche.Avancement, stUneInfoGantt.Tâche)
      	SINON
      		// Ajoute la ligne en fils
      		nIndiceLigne = TableAjouteFils(TABLE_Gantt, stUneInfoGantt.nIndiceParent, stUneInfoGantt.Tâche.ID, stUneInfoGantt.Tâche.Titre, stUneInfoGantt.Tâche.DateDébut, stUneInfoGantt.Tâche.DuréeEnJour, stUneInfoGantt.Tâche.Avancement, stUneInfoGantt.Tâche)
      	FIN
      	
      FIN
      
      // Applique les "styles" au niveau de la table (image, cellule grisé, etc)
      POUR TOUTE ligne nIndice de TABLE_Gantt
      	
      	// Récupère la tâche
      	cUneTache <- COL_Gantt[COL_IDTâche]
      	// Si la tâche est cumulative
      	SI cUneTache.Cumulative ALORS PasseTâcheEnCumulative(nIndice)
      	// Si la tâche est un jalon
      	SI cUneTache.Jalon ALORS PasseTâcheEnJalon(nIndice)
      	// Si la tâche n'est ni l'un ni l'autre
      	SI cUneTache.Cumulative = Faux _ET_ cUneTache.Jalon = Faux ALORS PasseTâcheEnStandard(nIndice)
      	
      FIN
      
      // Parcourt le tableau pour ajouter les liens
      POUR TOUT ELEMENT cUnLienGantt DE gstDiagrammeGantt.tabListeLiens
      	GanttAjouteLien(COL_Gantt, cUnLienGantt)
      FIN
      
      // Réorganise automatiquement les tâches
      SI INT_RéorganisationAutomatique ALORS GanttOrganiseTâche(COL_Gantt)
      
      // Met à jour les informations du diagramme de Gantt
      AfficheInfosGénéral()
      
      TABLE_Gantt..AffichageActif = Vrai
      
      // Modifie l'état des boutons
      AdapteBoutons()
     type : 458752
   -
     name : MetAJourTâchesSuiteRéorganisationAuto
     procedure_id : 1335883443533095343
     type_code : 14
     code : |1+
      // Résumé : Met à jour les informations des tâches dans la table suite à une réorganisation automatique
      // Syntaxe :
      //MetAJourTâchesSuiteRéorganisationAuto ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MetAJourTâchesSuiteRéorganisationAuto()
      
      UneTâche est une TâcheGantt
      
      
      // Seulement dans le cas d'une réorganisation automatique
      si COL_Gantt..RéorganisationAutomatique = faux alors retour
      
      TABLE_Gantt..AffichageActif = faux
      
      // Parcours les lignes de la table
      pour toute ligne nIndice de TABLE_Gantt
      
      	// Récupère la tâche
      	UneTâche = COL_Gantt[COL_IDTâche[nIndice]]
      	
      	// Ne pas prendre en compte les tâches cumulatives (seront calculées après)
      	si UneTâche.Cumulative alors continuer
      	
      	// Met à jour les informations de la tâche
      	COL_Avancement[nIndice] = UneTâche.avancement
      	COL_Durée[nIndice] = UneTâche.DuréeEnJour
      	COL_DateDébut[nIndice] = UneTâche.DateDébut
      	
      	// Recalcule les informations des parents
      	CalculTâchesParents(nIndice)
      	
      FIN
      
      TABLE_Gantt..AffichageActif = vrai
     type : 458752
   -
     name : AdapteBoutons
     procedure_id : 1336960960685838456
     type_code : 14
     code : |1-
      // Résumé : Adapte l'état des boutons au contenu du diagramme
      // Syntaxe :
      //AdapteBoutons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AdapteBoutons()
      
      // Si la table est vide
      si TABLE_Gantt..Occurrence = 0 ALORS
      
      	// Grise les boutons qui permettent de créer, sauver et imprimer
      	BTN_Nouveau..etat = grise
      	BTN_Sauver..Etat = Grisé
      	BTN_Imprimer..Etat = grise
      
      SINON // Si la table contient au moins une ligne
      	
      	// Rend actif les boutons qui permettent de créer, sauver et imprimer
      	BTN_Nouveau..Etat = Actif
      	BTN_Sauver..Etat = Actif
      	BTN_Imprimer..Etat = Actif
      	
      FIN
     type : 458752
